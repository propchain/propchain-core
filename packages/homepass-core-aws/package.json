{
  "name": "@homepass2018/aws",
  "version": "1.0.1",
  "license": "MIT",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "lint": "eslint \"src/**/*.{js,ts}\"",
    "lint:types": "tsc --noEmit",
    "test": "jest",
    "test:ci": "jest --reporters ./node_modules/jest-junit",
    "build": "rm -rf lib && npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "prepublish": "npm run build"
  },
  "peerDependencies": {
    "aws-sdk": "^2.382.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/plugin-proposal-class-properties": "^7.5.0",
    "@babel/plugin-proposal-numeric-separator": "^7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.5.0",
    "@babel/plugin-transform-regenerator": "^7.4.5",
    "@babel/preset-env": "^7.5.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.12",
    "aws-sdk": "^2.814.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.8.0",
    "eslint": "5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest": "^22.7.2",
    "eslint-plugin-prettier": "^3.1.0",
    "expect-more-jest": "^2.4.2",
    "jest": "^24.8.0",
    "jest-junit": "^6.4.0",
    "jest-watch-typeahead": "^0.3.1",
    "lerna": "^3.15.0",
    "prettier": "^1.18.2",
    "typescript": "~3.5.2"
  },
  "dependencies": {
    "http-status-codes": "^1.3.2"
  },
  "jest": {
    "verbose": false,
    "setupFilesAfterEnv": [
      "<rootDir>/test/jest.setup.js"
    ],
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "babel-jest"
    },
    "testMatch": [
      "**/__tests__/**/?(*.)+(spec).(js|ts)?(x)",
      "**/?(*.)+(spec).(js|ts)?(x)"
    ],
    "watchPlugins": [
      "jest-watch-typeahead/filename",
      "jest-watch-typeahead/testname"
    ]
  },
  "gitHead": "3a2e31fa7d25c2170b6d88b6fbadec3d899b8b38"
}
